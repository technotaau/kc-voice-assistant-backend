# API Keys
OPENAI_API_KEY=your_openai_api_key_here
PINECONE_API_KEY=your_pinecone_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Voice Configuration
KC_VOICE_ID=your_elevenlabs_voice_id_here

# Google Cloud credentials (for Speech-to-Text and Translation)
# Option 1: Path to credentials file (local development)
GOOGLE_APPLICATION_CREDENTIALS=path/to/credentials.json
# Option 2: JSON string (for cloud deployment)
GOOGLE_CREDENTIALS={"type":"service_account",...}

# Pinecone Configuration (Optional - only if using environment)
PINECONE_ENVIRONMENT=

# Database (Optional - for future use)
DATABASE_URL=

# Server Configuration
PORT=5000
NODE_ENV=production

# Frontend URL (for CORS)
FRONTEND_URL=https://your-app.vercel.app